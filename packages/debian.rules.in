#!/usr/bin/make -f
# -*- makefile -*-
# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	dh $@

override_dh_strip:
	dh_strip --dbg-package=@PACKAGE_NAME@-dbg

override_dh_auto_configure:
	cmake -DCMAKE_INSTALL_PREFIX:PATH=/usr -D CMAKE_BUILD_TYPE:String="Package" .

override_dh_auto_test:
	ctest -V
