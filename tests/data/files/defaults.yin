    <list name="list">
      <key value="name"/>
      <leaf name="name">
        <type name="string"/>
      </leaf>
      <leaf name="value">
        <type name="int32"/>
        <default value="42"/>
      </leaf>
    </list>
    <choice name="select">
      <default value="a"/>
      <case name="a">
        <choice name="a">
          <leaf name="a1">
            <type name="int32"/>
            <default value="42"/>
          </leaf>
          <leaf name="a2">
            <type name="int32"/>
            <default value="24"/>
          </leaf>
        </choice>
      </case>
      <leaf name="b">
        <type name="string"/>
      </leaf>
      <container name="c">
        <presence value=""/>
        <leaf name="x">
          <type name="int32"/>
          <default value="42"/>
        </leaf>
      </container>
    </choice>
    <choice name="select2">
      <default value="s2b"/>
      <leaf name="s2a">
        <type name="int32"/>
        <default value="42"/>
      </leaf>
      <case name="s2b">
        <choice name="s2b">
          <default value="b1"/>
