              <default value="42"/>
            </leaf>
            <leaf name="b1_2">
              <type name="string"/>
            </leaf>
          </case>
          <leaf name="b2">
            <type name="int32"/>
            <default value="42"/>
          </leaf>
        </choice>
      </case>
    </choice>
  </container>
  <container name="hidden">
    <leaf name="foo">
      <type name="int32"/>
      <default value="42"/>
    </leaf>
    <leaf name="baz">
      <type name="int32"/>
      <default value="42"/>
    </leaf>
  </container>
