<?xml version="1.0" encoding="UTF-8"?>
<module name="defaults"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:df="urn:libyang:tests:defaults">
  <namespace uri="urn:libyang:tests:defaults"/>
  <prefix value="df"/>
  <container name="df">
    <leaf name="foo">
      <type name="int32"/>
      <default value="42"/>
    </leaf>
    <container name="bar">
      <presence value=""/>
