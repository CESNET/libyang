        xmlns:common-deviation_mod="urn:common-deviation">
  <yang-version value="1.1"/>
  <namespace uri="urn:error-substatement12-deviation"/>
  <prefix value="error-substatement12-deviation_mod"/>

  <import module="common-deviation">
