<?xml version="1.0" encoding="UTF-8"?>
<module name="error-substatement14-deviation"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:error-substatement14-deviation_mod="urn:error-substatement14-deviation"
        xmlns:common-deviation_mod="urn:common-deviation">
  <yang-version value="1.1"/>
  <namespace uri="urn:error-substatement14-deviation"/>
  <prefix value="error-substatement14-deviation_mod"/>

  <import module="common-deviation">
    <prefix value="common-deviation_mod"/>
  </import>

  <deviation target-node="/common-deviation_mod:con/common-deviation_mod:leaf2">
    <deviate value="add">
      <!-- Adding the mandatory statement is forbidden in leaf statement with default statement -->
      <mandatory value="true"/>
    </deviate>
  </deviation>
</module>
